{
  "expo": {
    "name": "Lio",
    "slug": "lio",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "platforms": [
      "ios",
      "android"
    ],
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cisfransorganization.lio",
      "appleTeamId": "L8M5M9DCPC",
      "entitlements": {
        "com.apple.security.application-groups": [
          "group.com.cisfransorganization.lio"
        ]
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.cisfransorganization.lio"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "0d0caa8c-f6aa-41a8-befb-ca0c256bb062"
      }
    },
    "owner": "cisfrans-organization"
  }
}